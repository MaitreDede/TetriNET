<UserControl x:Class="TetriNET.WPF_WCF_Client.Controls.Inventory"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="300">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock x:Name="TxtInventory" HorizontalAlignment="Left" VerticalAlignment="Top" Width="Auto" Height="16" />
            <TextBlock x:Name="TxtFirstSpecial" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="Auto" Height="16"/>
        </StackPanel>
    </Grid>
</UserControl>
